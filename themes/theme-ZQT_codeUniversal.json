{
	"$schema": "vscode://schemas/color-theme",
	"name": "Universal Code Highlight - ZQT Theme",
	"//description": "通用的代码高亮",
	"tokenColors": [
		{
			"scope": "comment",
			"settings": {
				"foreground": "#717171"
			}
		},
		{
			"scope": "punctuation.definition.comment",
			"settings": {
				"foreground": "#717171"
			}
		},
		{
			"scope": "invalid",
			"settings": {
				"foreground": "#fc4734"
			}
		},
		{
			"scope": "entity.name.function",
			"settings": {
				"foreground": "#cca45e"
			}
		},
		{
			"scope": "support.function",
			"settings": {
				"foreground": "#dfa635"
			}
		},
		{
			"scope": "variable.language",
			"settings": {
				"foreground": "#75c59b"
			}
		},
		{
			"scope": "variable.parameter",
			"settings": {
				"foreground": "#96be8e"
			}
		},
		{
			"scope": "variable.other.property",
			"settings": {
				"foreground": "#b3ac80"
			}
		},
		{
			"scope": "support.variable.property",
			"settings": {
				"foreground": "#c3b351"
			}
		},
		{
			"scope": "variable",
			"settings": {
				"foreground": "#97b591"
			}
		},
		{
			"scope": "support.variable",
			"settings": {
				"foreground": "#87c47a"
			}
		},
		{
			"scope": "constant.language",
			"settings": {}
		},
		{
			"scope": "constant.numeric",
			"settings": {
				"foreground": "#bba3e8"
			}
		},
		{
			"scope": "string",
			"settings": {
				"foreground": "#c69db7"
			}
		},
		{
			"scope": "string.regexp",
			"settings": {
				"foreground": "#dd8cc3"
			}
		},
		{
			"scope": [
				"string.interpolated",
				"string.template"
			],
			"settings": {
				"foreground": "#dd8cc3"
			}
		},
		{
			"scope": [
				"constant.character",
				"punctuation.definition.entity"
			],
			"settings": {
				"foreground": "#dd8cc3"
			}
		},
		{
			"scope": "keyword.control",
			"settings": {
				"foreground": "#e88aa8"
			}
		},
		{
			"scope": "keyword.operator",
			"settings": {
				"foreground": "#dea08b"
			}
		},
		{
			"scope": "storage.type",
			"settings": {
				"foreground": "#9ea4e9"
			}
		},
		{
			"scope": "support.type",
			"settings": {
				"foreground": "#a2a8fc"
			}
		},
		{
			"scope": "entity.name.type",
			"settings": {
				"foreground": "#8cb2f1"
			}
		},
		{
			"scope": "punctuation.definition",
			"settings": {
				"foreground": "#83aac7"
			}
		},
		{
			"scope": "punctuation.separator",
			"settings": {
				"foreground": "#bebebe"
			}
		}
	]
}