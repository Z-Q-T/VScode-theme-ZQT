{
	"$schema": "vscode://schemas/color-theme",
	"name": "Universal Code Highlight - ZQT Theme",
	"//description": "通用的代码高亮",
	"include": "./interface_ZQT.json",
	"tokenColors": [
		{
			"scope": "class",
			"settings": {
				"foreground": "#FFF"
			}
		},
		{
			"scope": "comment",
			"settings": {
				"foreground": "#7f7f7f"
			}
		},
		{
			"scope": "constant",
			"settings": {
				"foreground": "#00ccb2"
			}
		},
		{
			"scope": "constant.language.boolean",
			"settings": {
				"foreground": "#f982aa"
			}
		},
		{
			"scope": "decorator",
			"settings": {
				"foreground": "#FFF"
			}
		},
		{
			"scope": "entity",
			"settings": {
				"foreground": "#dfa11a"
			}
		},
		{
			"scope": "enum",
			"settings": {
				"foreground": "#FFF"
			}
		},
		{
			"scope": "enumMember",
			"settings": {
				"foreground": "#FFF"
			}
		},
		{
			"scope": "event",
			"settings": {
				"foreground": "#FFF"
			}
		},
		{
			"scope": "function",
			"settings": {
				"foreground": "#FFF"
			}
		},
		{
			"scope": "interface",
			"settings": {
				"foreground": "#FFF"
			}
		},
		{
			"scope": "keyword",
			"settings": {
				"foreground": "#ff8386"
			}
		},
		{
			"scope": "keyword.operator",
			"settings": {
				"foreground": "#fc8a61"
			}
		},
		{
			"scope": "label",
			"settings": {
				"foreground": "#FFF"
			}
		},
		{
			"scope": "macro",
			"settings": {
				"foreground": "#FFF"
			}
		},
		{
			"scope": "meta",
			"settings": {
				"foreground": "#FFF"
			}
		},
		{
			"scope": "method",
			"settings": {
				"foreground": "#FFF"
			}
		},
		{
			"scope": "namespace",
			"settings": {
				"foreground": "#FFF"
			}
		},
		{
			"scope": "number",
			"settings": {
				"foreground": "#FFF"
			}
		},
		{
			"scope": "operator",
			"settings": {
				"foreground": "#FFF"
			}
		},
		{
			"scope": "parameter",
			"settings": {
				"foreground": "#FFF"
			}
		},
		{
			"scope": "property",
			"settings": {
				"foreground": "#FFF"
			}
		},
		{
			"scope": "regexp",
			"settings": {
				"foreground": "#FFF"
			}
		},
		{
			"scope": "storage",
			"settings": {
				"foreground": "#d995c2"
			}
		},
		{
			"scope": "string",
			"settings": {
				"foreground": "#72c298"
			}
		},
		{
			"scope": "struct",
			"settings": {
				"foreground": "#FFF"
			}
		},
		{
			"scope": "support",
			"settings": {
				"foreground": "#f982aa"
			}
		},
		{
			"scope": "type",
			"settings": {
				"foreground": "#FFF"
			}
		},
		{
			"scope": "typeParameter",
			"settings": {
				"foreground": "#FFF"
			}
		},
		{
			"scope": "variable",
			"settings": {
				"foreground": "#bfab8b"
			}
		}
	]
}