{
	"$schema": "vscode://schemas/color-theme",
	"name": "Universal Code Highlight - ZQT Theme",
	"//description": "通用的代码高亮",
	"include": "./interface_ZQT.json",
	"tokenColors": [
		{
			"scope": [
				"namespace",
				"class"
			],
			"settings": {
				"foreground": "#FFF"
			}
		},
		{
			"scope": "constant",
			"settings": {
				"foreground": "#FFF"
			}
		},
		{
			"scope": "comment",
			"settings": {
				"foreground": "#7f7f7f"
			}
		},
		{
			"scope": "decorator",
			"settings": {
				"foreground": "#FFF"
			}
		},
		{
			"scope": "entity",
			"settings": {
				"foreground": "#FFF"
			}
		},
		{
			"scope": "enum",
			"settings": {
				"foreground": "#FFF"
			}
		},
		{
			"scope": "enumMember",
			"settings": {
				"foreground": "#FFF"
			}
		},
		{
			"scope": "event",
			"settings": {
				"foreground": "#FFF"
			}
		},
		{
			"scope": "function",
			"settings": {
				"foreground": "#FFF"
			}
		},
		{
			"scope": "interface",
			"settings": {
				"foreground": "#FFF"
			}
		},
		{
			"scope": "keyword",
			"settings": {
				"foreground": "#ffa8fc"
			}
		},
		{
			"scope": "label",
			"settings": {
				"foreground": "#FFF"
			}
		},
		{
			"scope": "macro",
			"settings": {
				"foreground": "#FFF"
			}
		},
		{
			"scope": "method",
			"settings": {
				"foreground": "#FFF"
			}
		},
		{
			"scope": "meta",
			"settings": {
				"foreground": "#FFF"
			}
		},
		{
			"scope": "number",
			"settings": {
				"foreground": "#FFF"
			}
		},
		{
			"scope": "operator",
			"settings": {
				"foreground": "#FFF"
			}
		},
		{
			"scope": "parameter",
			"settings": {
				"foreground": "#FFF"
			}
		},
		{
			"scope": "property",
			"settings": {
				"foreground": "#FFF"
			}
		},
		{
			"scope": "regexp",
			"settings": {
				"foreground": "#FFF"
			}
		},
		{
			"scope": "storage",
			"settings": {
				"foreground": "#f4c947"
			}
		},
		{
			"scope": "string",
			"settings": {
				"foreground": "#b5c87d"
			}
		},
		{
			"scope": "struct",
			"settings": {
				"foreground": "#FFF"
			}
		},
		{
			"scope": "support",
			"settings": {
				"foreground": "#FFF"
			}
		},
		{
			"scope": "type",
			"settings": {
				"foreground": "#FFF"
			}
		},
		{
			"scope": "typeParameter",
			"settings": {
				"foreground": "#FFF"
			}
		},
		{
			"scope": "variable",
			"settings": {
				"foreground": "#6dd3c0"
			}
		}
	]
}