{
	"$schema": "vscode://schemas/color-theme",
	"name": "Specific Code Highlight - ZQT Theme",
	"//description": "各个语言专有的代码高亮",
	"include": "./theme-ZQT_codeHighlightUniversal.json",
	"tokenColors": [
		{
			"scope": [
				"entity.name.tag",
				"entity.name.tag.css"
			],
			"settings": {
				"foreground": "#a7ace4"
			}
		},
		{
			"scope": [
				"entity.name.tag.custom"
			],
			"settings": {
				"foreground": "#9a9fff"
			}
		},
		{
			"scope": "meta.attribute",
			"settings": {
				"foreground": "#96b874"
			}
		},
		{
			"scope": [
				"meta.attribute.class.html",
				"entity.other.attribute-name.class.css"
			],
			"settings": {
				"foreground": "#d2a249"
			}
		},
		{
			"scope": [
				"meta.attribute.id.html",
				"entity.other.attribute-name.id.css"
			],
			"settings": {
				"foreground": "#eb9070"
			}
		},
		{
			"scope": "punctuation.definition.entity.css",
			"settings": {
				"foreground": "#83aac7"
			}
		},
		{
			"scope": "entity.other.attribute-name.css",
			"settings": {
				"foreground": "#6fc082"
			}
		},
		{
			"scope": "entity.other.attribute-name.pseudo-element.css",
			"settings": {
				"foreground": "#c59fe1"
			}
		},
		{
			"scope": "entity.other.attribute-name.pseudo-class.css",
			"settings": {
				"foreground": "#b1b239"
			}
		},
		{
			"scope": "support.type.property-name",
			"settings": {
				"foreground": "#9eb38b"
			}
		},
		{
			"scope": "support.type.vendored.property-name.css",
			"settings": {
				"foreground": "#75bca3"
			}
		},
		{
			"scope": [
				"support.constant.property-value.css",
				"constant.numeric.css"
			],
			"settings": {
				"foreground": "#c69db7"
			}
		},
		{
			"scope": "support.function.misc.css",
			"settings": {
				"foreground": "#e696ae"
			}
		},
		{
			"scope": [
				"variable.css",
				"variable.argument.css"
			],
			"settings": {
				"foreground": "#ed8b8b"
			}
		},
		{
			"scope": [
				"meta.function-call.python"
			],
			"settings": {
				"foreground": "#cca45e"
			}
		},
		{
			"scope": [
				"meta.function-call.arguments.python"
			],
			"settings": {
				"foreground": "#96be8e"
			}
		},
		{
			"scope": "keyword.other.important.css",
			"settings": {
				"foreground": "#00c5ee"
			}
		}
	]
}