{
	"$schema": "vscode://schemas/color-theme",
	"name": "Universal Code Highlight - ZQT BRIGHT Theme",
	"//description": "通用的代码高亮",
	"tokenColors": [
		{
			"scope": "comment",
			"settings": {
				"foreground": "#9e9e9e"
			}
		},
		{
			"scope": "punctuation.definition.comment",
			"settings": {
				"foreground": "#9e9e9e"
			}
		},
		{
			"scope": "invalid",
			"settings": {
				"foreground": "#ca545a"
			}
		},
		{
			"scope": "entity.name.function",
			"settings": {
				"foreground": "#7347af"
			}
		},
		{
			"scope": "support.function",
			"settings": {
				"foreground": "#7b47bf"
			}
		},
		{
			"scope": "variable.language",
			"settings": {
				"foreground": "#00814b"
			}
		},
		{
			"scope": "variable.parameter",
			"settings": {
				"foreground": "#307c1b"
			}
		},
		{
			"scope": "variable.other.property",
			"settings": {}
		},
		{
			"scope": "support.variable.property",
			"settings": {}
		},
		{
			"scope": "variable",
			"settings": {
				"foreground": "#297613"
			}
		},
		{
			"scope": "support.variable",
			"settings": {
				"foreground": "#227e00"
			}
		},
		{
			"scope": "constant.language",
			"settings": {
				"foreground": "#0063c3"
			}
		},
		{
			"scope": "constant.numeric",
			"settings": {
				"foreground": "#794db6"
			}
		},
		{
			"scope": "string",
			"settings": {
				"foreground": "#a33460"
			}
		},
		{
			"scope": "string.regexp",
			"settings": {
				"foreground": "#9e3081"
			}
		},
		{
			"scope": [
				"string.interpolated",
				"string.template"
			],
			"settings": {
				"foreground": "#9e3081"
			}
		},
		{
			"scope": [
				"constant.character",
				"punctuation.definition.entity"
			],
			"settings": {
				"foreground": "#9e3081"
			}
		},
		{
			"scope": "keyword.control",
			"settings": {
				"foreground": "#ad215f"
			}
		},
		{
			"scope": "keyword.operator",
			"settings": {
				"foreground": "#b13e06"
			}
		},
		{
			"scope": "storage.type",
			"settings": {
				"foreground": "#544ec5"
			}
		},
		{
			"scope": "support.type",
			"settings": {
				"foreground": "#5a56c7"
			}
		},
		{
			"scope": "entity.name.type",
			"settings": {
				"foreground": "#2b64c3"
			}
		},
		{
			"scope": "punctuation.definition",
			"settings": {
				"foreground": "#0064a2"
			}
		},
		{
			"scope": "punctuation.separator",
			"settings": {}
		}
	]
}