{
	"$schema": "vscode://schemas/color-theme",
	"name": "Universal Code Highlight - ZQT BRIGHT Theme",
	"//description": "通用的代码高亮",
	"tokenColors": [
		{
			"scope": "entity.name.function",
			"settings": {
				"foreground": "#af2938"
			}
		},
		{
			"scope": "support.function",
			"settings": {
				"foreground": "#bd3743"
			}
		},
		{
			"scope": "variable.language",
			"settings": {
				"foreground": "#008663"
			}
		},
		{
			"scope": "variable",
			"settings": {
				"foreground": "#007858"
			}
		},
		{
			"scope": "support.variable",
			"settings": {
				"foreground": "#008663"
			}
		},
		{
			"scope": "variable.other.property",
			"settings": {
				"foreground": "#227702"
			}
		},
		{
			"scope": "support.variable.property",
			"settings": {
				"foreground": "#308319"
			}
		},
		{
			"scope": "constant.language",
			"settings": {
				"foreground": "#a53d9f"
			}
		},
		{
			"scope": "constant.numeric",
			"settings": {
				"foreground": "#953490"
			}
		},
		{
			"scope": "string",
			"settings": {
				"foreground": "#953490"
			}
		},
		{
			"scope": "string.regexp",
			"settings": {
				"foreground": "#a53d9f"
			}
		},
		{
			"scope": [
				"string.interpolated",
				"string.template"
			],
			"settings": {
				"foreground": "#a53d9f"
			}
		},
		{
			"scope": [
				"constant.character",
				"punctuation.definition.entity"
			],
			"settings": {
				"foreground": "#a53d9f"
			}
		},
		{
			"scope": "keyword.control",
			"settings": {
				"foreground": "#006fd1"
			}
		},
		{
			"scope": "storage.type",
			"settings": {
				"foreground": "#7a4aba"
			}
		},
		{
			"scope": "support.type",
			"settings": {
				"foreground": "#814ec6"
			}
		},
		{
			"scope": "keyword.operator",
			"settings": {
				"foreground": "#007d9e"
			}
		},
		{
			"scope": "punctuation.definition",
			"settings": {
				"foreground": "#ad215f"
			}
		},
		{
			"scope": "punctuation.separator",
			"settings": {
				"foreground": "#6f6f6f"
			}
		},
		{
			"scope": "comment",
			"settings": {
				"foreground": "#ababab"
			}
		},
		{
			"scope": "punctuation.definition.comment",
			"settings": {
				"foreground": "#ababab"
			}
		},
		{
			"scope": "invalid",
			"settings": {
				"foreground": "#d64651"
			}
		}
	]
}