{
	"$schema": "vscode://schemas/color-theme",
	"name": "Universal Code Highlight - ZQT BRIGHT Theme",
	"//description": "通用的代码高亮",
	"tokenColors": [
		{
			"scope": [
				"entity.name.function",
				"meta.selector.css",
				"meta.function-call"
			],
			"settings": {
				"foreground": "#ae2c77"
			}
		},
		{
			"scope": "support.function",
			"settings": {
				"foreground": "#bc3a83"
			}
		},
		{
			"scope": [
				"variable",
				"meta.function-call.arguments"
			],
			"settings": {
				"foreground": "#007f5e"
			}
		},
		{
			"scope": [
				"support.variable",
				"support.type.property-name.css",
				"entity.other.attribute-name.html"
			],
			"settings": {
				"foreground": "#008c68"
			}
		},
		{
			"scope": "variable.language",
			"settings": {
				"foreground": "#008c68"
			}
		},
		{
			"scope": [
				"variable.other.property",
				"entity.other.attribute-name.pseudo-element.css",
				"entity.other.attribute-name.pseudo-class.css",
				"meta.attribute"
			],
			"settings": {
				"foreground": "#797a08"
			}
		},
		{
			"scope": "support.variable.property",
			"settings": {
				"foreground": "#7f8015"
			}
		},
		{
			"scope": [
				"string",
				"constant.numeric",
				"meta.item-access.arguments"
			],
			"settings": {
				"foreground": "#714ca6"
			}
		},
		{
			"scope": [
				"string.regexp",
				"string.interpolated",
				"string.template",
				"constant.character",
				"punctuation.definition.entity",
				"support.constant.property-value.css"
			],
			"settings": {
				"foreground": "#814ec6"
			}
		},
		{
			"scope": "constant.language",
			"settings": {
				"foreground": "#8c5ad3"
			}
		},
		{
			"scope": "keyword.control",
			"settings": {
				"foreground": "#a7611b"
			}
		},
		{
			"scope": "storage.type",
			"settings": {
				"foreground": "#007dc5"
			}
		},
		{
			"scope": "support.type",
			"settings": {
				"foreground": "#007dc5"
			}
		},
		{
			"scope": "keyword.operator",
			"settings": {
				"foreground": "#a54d6c"
			}
		},
		{
			"scope": "punctuation.definition",
			"settings": {
				"foreground": "#ad215f"
			}
		},
		{
			"scope": "punctuation.separator",
			"settings": {
				"foreground": "#6f6f6f"
			}
		},
		{
			"scope": "comment",
			"settings": {
				"foreground": "#ababab"
			}
		},
		{
			"scope": "punctuation.definition.comment",
			"settings": {
				"foreground": "#ababab"
			}
		},
		{
			"scope": "invalid",
			"settings": {
				"foreground": "#d64651"
			}
		}
	]
}