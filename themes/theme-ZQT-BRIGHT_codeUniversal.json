{
	"$schema": "vscode://schemas/color-theme",
	"name": "Universal Code Highlight - ZQT BRIGHT Theme",
	"//description": "通用的代码高亮",
	"tokenColors": [
		{
			"scope": "comment",
			"settings": {
				"foreground": "#717171"
			}
		},
		{
			"scope": "punctuation.definition.comment",
			"settings": {
				"foreground": "#717171"
			}
		},
		{
			"scope": "invalid",
			"settings": {
				"foreground": "#fc4734"
			}
		},
		{
			"scope": "entity.name.function",
			"settings": {}
		},
		{
			"scope": "support.function",
			"settings": {}
		},
		{
			"scope": "variable.language",
			"settings": {
				"foreground": "#17895b"
			}
		},
		{
			"scope": "variable.parameter",
			"settings": {
				"foreground": "#4f8444"
			}
		},
		{
			"scope": "variable.other.property",
			"settings": {
				"foreground": "#7c6f1e"
			}
		},
		{
			"scope": "support.variable.property",
			"settings": {}
		},
		{
			"scope": "variable",
			"settings": {
				"foreground": "#4e7d43"
			}
		},
		{
			"scope": "support.variable",
			"settings": {
				"foreground": "#478638"
			}
		},
		{
			"scope": "constant.language",
			"settings": {
				"foreground": "#1b71bc"
			}
		},
		{
			"scope": "constant.numeric",
			"settings": {
				"foreground": "#8062b0"
			}
		},
		{
			"scope": "string",
			"settings": {
				"foreground": "#955681"
			}
		},
		{
			"scope": "string.regexp",
			"settings": {
				"foreground": "#9f4d86"
			}
		},
		{
			"scope": [
				"string.interpolated",
				"string.template"
			],
			"settings": {
				"foreground": "#9f4d86"
			}
		},
		{
			"scope": [
				"constant.character",
				"punctuation.definition.entity"
			],
			"settings": {
				"foreground": "#9f4d86"
			}
		},
		{
			"scope": "keyword.control",
			"settings": {
				"foreground": "#a9496b"
			}
		},
		{
			"scope": "keyword.operator",
			"settings": {
				"foreground": "#aa5c40"
			}
		},
		{
			"scope": "storage.type",
			"settings": {
				"foreground": "#6265b3"
			}
		},
		{
			"scope": "support.type",
			"settings": {
				"foreground": "#6769bf"
			}
		},
		{
			"scope": "entity.name.type",
			"settings": {
				"foreground": "#4974ba"
			}
		},
		{
			"scope": "punctuation.definition",
			"settings": {
				"foreground": "#27709c"
			}
		},
		{
			"scope": "punctuation.separator",
			"settings": {
				"foreground": "#bebebe"
			}
		}
	]
}