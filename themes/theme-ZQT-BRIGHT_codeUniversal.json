{
	"$schema": "vscode://schemas/color-theme",
	"name": "Universal Code Highlight - ZQT BRIGHT Theme",
	"//description": "通用的代码高亮",
	"tokenColors": [
		{
			"scope": "comment",
			"settings": {
				"foreground": "#717171"
			}
		},
		{
			"scope": "punctuation.definition.comment",
			"settings": {
				"foreground": "#717171"
			}
		},
		{
			"scope": "invalid",
			"settings": {
				"foreground": "#fc4734"
			}
		},
		{
			"scope": "entity.name.function",
			"settings": {}
		},
		{
			"scope": "support.function",
			"settings": {}
		},
		{
			"scope": "variable.language",
			"settings": {
				"foreground": "#009962"
			}
		},
		{
			"scope": "variable.parameter",
			"settings": {
				"foreground": "#57914a"
			}
		},
		{
			"scope": "variable.other.property",
			"settings": {
				"foreground": "#887b2c"
			}
		},
		{
			"scope": "support.variable.property",
			"settings": {}
		},
		{
			"scope": "variable",
			"settings": {
				"foreground": "#59894f"
			}
		},
		{
			"scope": "support.variable",
			"settings": {
				"foreground": "#449630"
			}
		},
		{
			"scope": "constant.language",
			"settings": {
				"foreground": "#007cdf"
			}
		},
		{
			"scope": "constant.numeric",
			"settings": {
				"foreground": "#8e6ac7"
			}
		},
		{
			"scope": "string",
			"settings": {
				"foreground": "#a1628d"
			}
		},
		{
			"scope": "string.regexp",
			"settings": {
				"foreground": "#b54d98"
			}
		},
		{
			"scope": [
				"string.interpolated",
				"string.template"
			],
			"settings": {
				"foreground": "#b54d98"
			}
		},
		{
			"scope": [
				"constant.character",
				"punctuation.definition.entity"
			],
			"settings": {
				"foreground": "#b54d98"
			}
		},
		{
			"scope": "keyword.control",
			"settings": {
				"foreground": "#c24976"
			}
		},
		{
			"scope": "keyword.operator",
			"settings": {
				"foreground": "#bb6546"
			}
		},
		{
			"scope": "storage.type",
			"settings": {
				"foreground": "#6c6ecb"
			}
		},
		{
			"scope": "support.type",
			"settings": {
				"foreground": "#7071dd"
			}
		},
		{
			"scope": "entity.name.type",
			"settings": {
				"foreground": "#4c7fd3"
			}
		},
		{
			"scope": "punctuation.definition",
			"settings": {
				"foreground": "#347ca9"
			}
		},
		{
			"scope": "punctuation.separator",
			"settings": {
				"foreground": "#bebebe"
			}
		}
	]
}