{
	"$schema": "vscode://schemas/color-theme",
	"name": "Universal Code Highlight - ZQT BRIGHT Theme",
	"//description": "通用的代码高亮",
	"tokenColors": [
		{
			"scope": "comment",
			"settings": {
				"foreground": "#8f8f8f"
			}
		},
		{
			"scope": "punctuation.definition.comment",
			"settings": {
				"foreground": "#8f8f8f"
			}
		},
		{
			"scope": "invalid",
			"settings": {
				"foreground": "#ca545a"
			}
		},
		{
			"scope": "entity.name.function",
			"settings": {
				"foreground": "#6c5594"
			}
		},
		{
			"scope": "support.function",
			"settings": {
				"foreground": "#7652ac"
			}
		},
		{
			"scope": "variable.language",
			"settings": {
				"foreground": "#277a55"
			}
		},
		{
			"scope": "variable.parameter",
			"settings": {
				"foreground": "#507448"
			}
		},
		{
			"scope": "variable.other.property",
			"settings": {}
		},
		{
			"scope": "support.variable.property",
			"settings": {}
		},
		{
			"scope": "variable",
			"settings": {}
		},
		{
			"scope": "support.variable",
			"settings": {
				"foreground": "#3f7932"
			}
		},
		{
			"scope": "constant.language",
			"settings": {
				"foreground": "#0465af"
			}
		},
		{
			"scope": "constant.numeric",
			"settings": {
				"foreground": "#725b9a"
			}
		},
		{
			"scope": "string",
			"settings": {}
		},
		{
			"scope": "string.regexp",
			"settings": {
				"foreground": "#8f4478"
			}
		},
		{
			"scope": [
				"string.interpolated",
				"string.template"
			],
			"settings": {
				"foreground": "#8f4478"
			}
		},
		{
			"scope": [
				"constant.character",
				"punctuation.definition.entity"
			],
			"settings": {
				"foreground": "#8f4478"
			}
		},
		{
			"scope": "keyword.control",
			"settings": {
				"foreground": "#a62f5f"
			}
		},
		{
			"scope": "keyword.operator",
			"settings": {
				"foreground": "#99533b"
			}
		},
		{
			"scope": "storage.type",
			"settings": {
				"foreground": "#595c9b"
			}
		},
		{
			"scope": "support.type",
			"settings": {
				"foreground": "#5c5fac"
			}
		},
		{
			"scope": "entity.name.type",
			"settings": {
				"foreground": "#4669a2"
			}
		},
		{
			"scope": "punctuation.definition",
			"settings": {}
		},
		{
			"scope": "punctuation.separator",
			"settings": {
				"foreground": "#555"
			}
		}
	]
}