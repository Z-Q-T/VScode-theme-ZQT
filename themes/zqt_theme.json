{
	"$schema": "vscode://schemas/color-theme",
	"name": "ZQT Theme",
	"//description":"各个语言专有的代码高亮",
	"include": "./dark_vs_ZQT.json",
	"tokenColors": [

		{
			"scope": [
				"support.function.misc.css",
				"support.function.gradient.css",
				"support.function.url.css",
				"support.function.transform.css",
				"support.function.calc.css",
			],
			"settings": {
				"foreground": "#41a8fc"
			}
		},
		{
			"scope": [
				"variable.css",
				"variable.scss",
				"variable.argument.css",
			],
			"settings": {
				"foreground": "#fad186"
			}
		},
		{
			"scope": "entity.name.tag.css",
			"settings": {
				"foreground": "#5ecab8"
			}
		},
		{
			"scope": [
				"entity.other.attribute-name.class.css",
				"entity.other.attribute-name.class.mixin.css",
				"entity.other.attribute-name.parent-selector.css",
				"source.css.less entity.other.attribute-name.id",
				"entity.other.attribute-name.attribute.scss",
				"entity.other.attribute-name.scss"
			],
			"settings": {
				"foreground": "#e091c6"
			}
		},
		{
			"scope": [
				"entity.other.attribute-name.id.css",
			],
			"settings": {
				"foreground": "#f05cbe"
			}
		},
		{
			"scope": [
				"entity.other.attribute-name.pseudo-class.css",
			],
			"settings": {
				"foreground": "#b09aff"
			}
		},
		{
			"scope": [
				"entity.other.attribute-name.pseudo-element.css",
			],
			"settings": {
				"foreground": "#a8c9a4"
			}
		},
		{
			"scope": [
				"support.type.property-name.css",
			],
			"settings": {
				"foreground": "#a8cff3"
			}
		},
		{
			"scope": [
				"support.type.vendored.property-name.css",
			],
			"settings": {
				"foreground": "#c6e0ee"
			}
		},
		{
			"scope": "keyword.other.important.css",
			"settings": {
				"fontStyle": "italic",
				"foreground": "#adffad"
			}
		},
		{
			"name": "CSS property value",
			"scope": [
				"support.constant.property-value",
				"support.constant.font-name",
				"support.constant.media-type",
				"support.constant.media",
				"constant.other.rgb-value",
				"support.constant.color"
			],
			"settings": {
				"foreground": "#ffb3fb"
			}
		},
		{
			"name": "CSS property value",
			"scope": [
				"constant.other.color.rgb-value",
			],
			"settings": {
				"foreground": "#f89e74"
			}
		},


		{
			"name": "Class Variable",
			"scope": "variable.other.object.property.js",
			"settings": { "fontStyle": "", "foreground": "#65c7d2" }
		},
		{
			"name": "Class Variable",
			"scope": "variable.other.object.js",
			"settings": { "fontStyle": "", "foreground": "#7bddb4" }
		},
		{
			"name": "Class Variable",
			"scope": "storage.type.function.js",
			"settings": { "fontStyle": "", "foreground": "#e5ffa5" }
		},
		{
			"name": "Class Variable",
			"scope": "variable.other.readwrite.js",
			"settings": { "fontStyle": "", "foreground": "#f9b7e5" }
		},
		{
			"name": "Class Variable",
			"scope": "keyword.control.conditional.js",
			"settings": { "fontStyle": "", "foreground": "#b5f071" }
		},


		{
			"scope": "meta.structure.dictionary.key.python",
			"settings": {
				"foreground": "#9cdcfe"
			}
		},	 
		{
			"scope": "variable.parameter.function.language.special.self.python",
			"settings": {
				"foreground": "#a1a1ff"
			}
		},
		{
			"scope": "variable.language.special.self.python",
			"settings": {
				"foreground": "#a1a1ff"
			}
		},
		{
			"scope": "meta.function-call.generic.python",
			"settings": {
				"foreground": "#e77ff3"
			}
		},
		{
			"scope": "meta.attribute.python",
			"settings": {
				"foreground": "#9fff90"
			}
		},
		{
			"scope": "meta.member.access.python",
			"settings": {
				"foreground": "#9fff90"
			}
		},
		{
			"scope": "meta.function-call.arguments.python",
			"settings": {
				"foreground": "#e7e188"
			}
		},
		{
			"name": "Types declaration and references",
			"scope": [
				"meta.return-type",
				"support.class",
				"support.type",
				"entity.name.type",
				"entity.name.namespace",
				"entity.other.attribute",
				"entity.name.scope-resolution",
				"entity.name.class",
				"storage.type.numeric.go",
				"storage.type.byte.go",
				"storage.type.boolean.go",
				"storage.type.string.go",
				"storage.type.uintptr.go",
				"storage.type.error.go",
				"storage.type.rune.go",
				"storage.type.cs",
				"storage.type.generic.cs",
				"storage.type.modifier.cs",
				"storage.type.variable.cs",
				"storage.type.annotation.java",
				"storage.type.generic.java",
				"storage.type.java",
				"storage.type.object.array.java",
				"storage.type.primitive.array.java",
				"storage.type.primitive.java",
				"storage.type.token.java",
				"storage.type.groovy",
				"storage.type.annotation.groovy",
				"storage.type.parameters.groovy",
				"storage.type.generic.groovy",
				"storage.type.object.array.groovy",
				"storage.type.primitive.array.groovy",
				"storage.type.primitive.groovy"
			],
			"settings": {
				"foreground": "#bee424"
			}
		},


		{
			"name": "Object keys, TS grammar specific",
			"scope": [
				"meta.object-literal.key"
			],
			"settings": {
				"foreground": "#9CDCFE"
			}
		}
	]
}