{
	"$schema": "vscode://schemas/color-theme",
	"name": "Universal Code Highlight - ZQT Theme",
	"//description": "通用的代码高亮",
	"include": "./theme-ZQT_interface.json",
	"tokenColors": [
		{
			"scope": "comment",
			"settings": {
				"foreground": "#717171"
			}
		},
		{
			"scope": "punctuation.definition.comment",
			"settings": {
				"foreground": "#717171"
			}
		},
		{
			"scope": "invalid",
			"settings": {
				"foreground": "#fc4734"
			}
		},
		{
			"scope": "entity.name.function",
			"settings": {
				"foreground": "#c69e58"
			}
		},
		{
			"scope": "support.function",
			"settings": {
				"foreground": "#d9a02c"
			}
		},
		{
			"scope": "variable.language",
			"settings": {
				"foreground": "#6fbe95"
			}
		},
		{
			"scope": "variable.parameter",
			"settings": {
				"foreground": "#90b888"
			}
		},
		{
			"scope": "variable.other.property",
			"settings": {
				"foreground": "#ada67a"
			}
		},
		{
			"scope": "support.variable.property",
			"settings": {
				"foreground": "#bdac4a"
			}
		},
		{
			"scope": "variable",
			"settings": {
				"foreground": "#91ae8b"
			}
		},
		{
			"scope": "support.variable",
			"settings": {
				"foreground": "#81bd74"
			}
		},
		{
			"scope": "constant.language",
			"settings": {
				"foreground": "#6fa4fc"
			}
		},
		{
			"scope": "constant.numeric",
			"settings": {
				"foreground": "#b49ce1"
			}
		},
		{
			"scope": "string",
			"settings": {
				"foreground": "#bf96b0"
			}
		},
		{
			"scope": "string.regexp",
			"settings": {
				"foreground": "#d686bc"
			}
		},
		{
			"scope": [
				"string.interpolated",
				"string.template"
			],
			"settings": {
				"foreground": "#d686bc"
			}
		},
		{
			"scope": [
				"constant.character",
				"punctuation.definition.entity"
			],
			"settings": {
				"foreground": "#d686bc"
			}
		},
		{
			"scope": "keyword.control",
			"settings": {
				"foreground": "#e183a1"
			}
		},
		{
			"scope": "keyword.operator",
			"settings": {
				"foreground": "#d79a85"
			}
		},
		{
			"scope": "storage.type",
			"settings": {
				"foreground": "#989ee2"
			}
		},
		{
			"scope": "support.type",
			"settings": {
				"foreground": "#9a9fff"
			}
		},
		{
			"scope": "entity.name.type",
			"settings": {
				"foreground": "#86acea"
			}
		},
		{
			"scope": "punctuation.definition",
			"settings": {
				"foreground": "#7da4c0"
			}
		},
		{
			"scope": "punctuation.separator",
			"settings": {
				"foreground": "#bebebe"
			}
		}
	]
}