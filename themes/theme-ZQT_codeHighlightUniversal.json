{
	"$schema": "vscode://schemas/color-theme",
	"name": "Universal Code Highlight - ZQT Theme",
	"//description": "通用的代码高亮",
	"include": "./theme-ZQT_interface.json",
	"tokenColors": [
		{
			"scope": "comment",
			"settings": {
				"foreground": "#636363"
			}
		},
		{
			"scope": "invalid",
			"settings": {
				"foreground": "#ff3b3b"
			}
		},
		{
			"scope": "constant.numeric",
			"settings": {
				"foreground": "#14bbc2"
			}
		},
		{
			"scope": "constant.character",
			"settings": {
				"foreground": "#57bc8a"
			}
		},
		{
			"scope": "constant.language",
			"settings": {
				"foreground": "#ea7ca1"
			}
		},
		{
			"scope": "entity",
			"settings": {
				"foreground": "#e38d3d"
			}
		},
		{
			"scope": "entity.name.function",
			"settings": {
				"foreground": "#e38d3d"
			}
		},
		{
			"scope": "entity.name.tag",
			"settings": {
				"foreground": "#e38d3d"
			}
		},
		{
			"scope": "entity.other.attribute-name",
			"settings": {
				"foreground": "#e38d3d"
			}
		},
		{
			"scope": "keyword",
			"settings": {
				"foreground": "#ef7e80"
			}
		},
		{
			"scope": "keyword.control",
			"settings": {
				"foreground": "#ef7e80"
			}
		},
		{
			"scope": "keyword.operator",
			"settings": {
				"foreground": "#ed835e"
			}
		},
		{
			"scope": "meta",
			"settings": {
				"foreground": "#55aee8"
			}
		},
		{
			"scope": "punctuation",
			"settings": {
				"foreground": "#bebebe"
			}
		},
		{
			"scope": "punctuation.definition.comment",
			"settings": {
				"foreground": "#636363"
			}
		},
		{
			"scope": "storage",
			"settings": {
				"foreground": "#cf8cb8"
			}
		},
		{
			"scope": "string",
			"settings": {
				"foreground": "#77b493"
			}
		},
		{
			"scope": "string.regexp",
			"settings": {
				"foreground": "#FFF"
			}
		},
		{
			"scope": "string.interpolated",
			"settings": {
				"foreground": "#3fbf86"
			}
		},
		{
			"scope": "support",
			"settings": {
				"foreground": "#ea7ca1"
			}
		},
		{
			"scope": "support.function",
			"settings": {
				"foreground": "#ea7ca1"
			}
		},
		{
			"scope": "variable",
			"settings": {
				"foreground": "#c59c6d"
			}
		},
		{
			"scope": "variable.parameter",
			"settings": {
				"foreground": "#bf9f6a"
			}
		},
		{
			"scope": "variable.language",
			"settings": {
				"foreground": "#bf9f6a"
			}
		}
	]
}