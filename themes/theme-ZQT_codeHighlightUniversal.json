{
	"$schema": "vscode://schemas/color-theme",
	"name": "Universal Code Highlight - ZQT Theme",
	"//description": "通用的代码高亮",
	"include": "./theme-ZQT_interface.json",
	"tokenColors": [
		{
			"scope": "comment",
			"settings": {
				"foreground": "#636363"
			}
		},
		{
			"scope": "invalid",
			"settings": {
				"foreground": "#ff3b3b"
			}
		},
		{
			"scope": "constant.numeric",
			"settings": {
				"foreground": "#2fbda7"
			}
		},
		{
			"scope": "constant.character",
			"settings": {
				"foreground": "#57bc8a"
			}
		},
		{
			"scope": "constant.language",
			"settings": {
				"foreground": "#ea7ca1"
			}
		},
		{
			"scope": "entity",
			"settings": {
				"foreground": "#e38d3d"
			}
		},
		{
			"scope": "entity.name.function",
			"settings": {
				"foreground": "#dc932e"
			}
		},
		{
			"scope": "entity.name.tag",
			"settings": {
				"foreground": "#d48aad"
			}
		},
		{
			"scope": "entity.other.attribute-name",
			"settings": {
				"foreground": "#c59c6d"
			}
		},
		{
			"scope": "keyword",
			"settings": {
				"foreground": "#ef7e80"
			}
		},
		{
			"scope": "keyword.control",
			"settings": {
				"foreground": "#e68677"
			}
		},
		{
			"scope": "keyword.operator",
			"settings": {
				"foreground": "#f58397"
			}
		},
		{
			"scope": "meta",
			"settings": {
				"foreground": "#6eaecd"
			}
		},
		{
			"scope": "punctuation",
			"settings": {
				"foreground": "#88a8c9"
			}
		},
		{
			"scope": "punctuation.definition.comment",
			"settings": {
				"foreground": "#636363"
			}
		},
		{
			"scope": "punctuation.definition.entity",
			"settings": {
				"foreground": "#57bc8a"
			}
		},
		{
			"scope": "storage",
			"settings": {
				"foreground": "#cf8cb8"
			}
		},
		{
			"scope": "string",
			"settings": {
				"foreground": "#77b493"
			}
		},
		{
			"scope": "string.regexp",
			"settings": {
				"foreground": "#c690ec"
			}
		},
		{
			"scope": "string.interpolated",
			"settings": {
				"foreground": "#a494f6"
			}
		},
		{
			"scope": "support",
			"settings": {
				"foreground": "#ea7ca1"
			}
		},
		{
			"scope": "support.function",
			"settings": {
				"foreground": "#ea7ca1"
			}
		},
		{
			"scope": "variable",
			"settings": {
				"foreground": "#bf9f6a"
			}
		},
		{
			"scope": "variable.parameter",
			"settings": {
				"foreground": "#b0a66b"
			}
		},
		{
			"scope": "variable.language",
			"settings": {}
		}
	]
}