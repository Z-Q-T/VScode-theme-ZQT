{
	"$schema": "vscode://schemas/color-theme",
	"name": "Universal Code Highlight - ZQT Theme",
	"//description": "通用的代码高亮",
	"include": "./theme-ZQT_interface.json",
	"tokenColors": [
		{
			"scope": "comment",
			"settings": {
				"foreground": "#636363"
			}
		},
		{
			"scope": "punctuation.definition.comment",
			"settings": {
				"foreground": "#636363"
			}
		},
		{
			"scope": "constant.character",
			"settings": {
				"foreground": "#39c3ae"
			}
		},
		{
			"scope": "punctuation.definition.entity",
			"settings": {
				"foreground": "#39c3ae"
			}
		},
		{
			"scope": "invalid",
			"settings": {
				"foreground": "#fc4734"
			}
		},
		{
			"scope": "constant.language",
			"settings": {
				"foreground": "#839ffc"
			}
		},
		{
			"scope": "constant.numeric",
			"settings": {
				"foreground": "#1db8ce"
			}
		},
		{
			"scope": "entity.name.function",
			"settings": {
				"foreground": "#e1957a"
			}
		},
		{
			"scope": "keyword.control",
			"settings": {
				"foreground": "#e183a1"
			}
		},
		{
			"scope": "keyword.operator",
			"settings": {
				"foreground": "#9ea4e9"
			}
		},
		{
			"scope": "keyword",
			"settings": {
				"foreground": "#59aaf8"
			}
		},
		{
			"scope": "punctuation.definition",
			"settings": {
				"foreground": "#c5ad86"
			}
		},
		{
			"scope": "punctuation.separator",
			"settings": {
				"foreground": "#bebebe"
			}
		},
		{
			"scope": "storage",
			"settings": {
				"foreground": "#c68bd3"
			}
		},
		{
			"scope": "string.interpolated",
			"settings": {
				"foreground": "#47c58c"
			}
		},
		{
			"scope": "string.regexp",
			"settings": {
				"foreground": "#79c069"
			}
		},
		{
			"scope": "string",
			"settings": {
				"foreground": "#8ab091"
			}
		},
		{
			"scope": "support.function",
			"settings": {
				"foreground": "#ed8b8b"
			}
		},
		{
			"scope": "support.type",
			"settings": {
				"foreground": "#d99c68"
			}
		},
		{
			"scope": "support",
			"settings": {
				"foreground": "#bdac4a"
			}
		},
		{
			"scope": "variable.language",
			"settings": {
				"foreground": "#a3b46b"
			}
		},
		{
			"scope": "variable.parameter",
			"settings": {
				"foreground": "#d3a062"
			}
		},
		{
			"scope": "variable",
			"settings": {
				"foreground": "#bf9f6a"
			}
		}
	]
}