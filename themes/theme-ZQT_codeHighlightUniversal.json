{
	"$schema": "vscode://schemas/color-theme",
	"name": "Universal Code Highlight - ZQT Theme",
	"//description": "通用的代码高亮",
	"include": "./theme-ZQT_interface.json",
	"tokenColors": [
		{
			"scope": "comment",
			"settings": {
				"foreground": "#636363"
			}
		},
		{
			"scope": "punctuation.definition.comment",
			"settings": {
				"foreground": "#636363"
			}
		},
		{
			"scope": "constant.character",
			"settings": {
				"foreground": "#39c3ae"
			}
		},
		{
			"scope": "punctuation.definition.entity",
			"settings": {
				"foreground": "#39c3ae"
			}
		},
		{
			"scope": "invalid",
			"settings": {
				"foreground": "#fc4734"
			}
		},
		{
			"scope": "constant.language",
			"settings": {
				"foreground": "#839ffc"
			}
		},
		{
			"scope": "constant.numeric",
			"settings": {
				"foreground": "#1db8ce"
			}
		},
		{
			"scope": "entity.name.function",
			"settings": {
				"foreground": "#e1957a"
			}
		},
		{
			"scope": "entity.name.tag",
			"settings": {
				"foreground": "#bb95c4"
			}
		},
		{
			"scope": "entity.other.attribute-name",
			"settings": {
				"foreground": "#bf9f6a"
			}
		},
		{
			"scope": "entity",
			"settings": {
				"foreground": "#48b7bd"
			}
		},
		{
			"scope": "keyword.control",
			"settings": {
				"foreground": "#e183a1"
			}
		},
		{
			"scope": "keyword.operator",
			"settings": {
				"foreground": "#9ea4e9"
			}
		},
		{
			"scope": "keyword",
			"settings": {
				"foreground": "#ef7e80"
			}
		},
		{
			"scope": "meta",
			"settings": {
				"foreground": "#c19c7f"
			}
		},
		{
			"scope": "punctuation",
			"settings": {
				"foreground": "#88a8c9"
			}
		},
		{
			"scope": "storage",
			"settings": {
				"foreground": "#c68bd3"
			}
		},
		{
			"scope": "string.interpolated",
			"settings": {
				"foreground": "#47c58c"
			}
		},
		{
			"scope": "string.regexp",
			"settings": {
				"foreground": "#79c069"
			}
		},
		{
			"scope": "string",
			"settings": {
				"foreground": "#8ab091"
			}
		},
		{
			"scope": "support.function",
			"settings": {
				"foreground": "#ea7ca1"
			}
		},
		{
			"scope": "support",
			"settings": {
				"foreground": "#ea7ca1"
			}
		},
		{
			"scope": "variable.language",
			"settings": {}
		},
		{
			"scope": "variable.parameter",
			"settings": {
				"foreground": "#b0a66b"
			}
		},
		{
			"scope": "variable",
			"settings": {
				"foreground": "#bf9f6a"
			}
		}
	]
}