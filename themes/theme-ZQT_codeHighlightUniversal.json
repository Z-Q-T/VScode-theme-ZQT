{
	"$schema": "vscode://schemas/color-theme",
	"name": "Universal Code Highlight - ZQT Theme",
	"//description": "通用的代码高亮",
	"include": "./interface_ZQT.json",
	"tokenColors": [
		{
			"scope": "class",
			"settings": {
				"foreground": "#FFF"
			}
		},
		{
			"scope": "comment",
			"settings": {
				"foreground": "#7f7f7f"
			}
		},
		{
			"scope": "constant",
			"settings": {
				"foreground": "#00c7ae"
			}
		},
		{
			"scope": "constant.language.boolean",
			"settings": {
				"foreground": "#f183a7"
			}
		},
		{
			"scope": "decorator",
			"settings": {
				"foreground": "#FFF"
			}
		},
		{
			"scope": "entity",
			"settings": {
				"foreground": "#ea9344"
			}
		},
		{
			"scope": "enum",
			"settings": {
				"foreground": "#FFF"
			}
		},
		{
			"scope": "enumMember",
			"settings": {
				"foreground": "#FFF"
			}
		},
		{
			"scope": "event",
			"settings": {
				"foreground": "#FFF"
			}
		},
		{
			"scope": "function",
			"settings": {
				"foreground": "#FFF"
			}
		},
		{
			"scope": "interface",
			"settings": {
				"foreground": "#FFF"
			}
		},
		{
			"scope": "keyword",
			"settings": {
				"foreground": "#f68486"
			}
		},
		{
			"scope": "keyword.operator",
			"settings": {
				"foreground": "#f48a64"
			}
		},
		{
			"scope": "label",
			"settings": {
				"foreground": "#FFF"
			}
		},
		{
			"scope": "macro",
			"settings": {
				"foreground": "#FFF"
			}
		},
		{
			"scope": "meta",
			"settings": {
				"foreground": "#FFF"
			}
		},
		{
			"scope": "method",
			"settings": {
				"foreground": "#FFF"
			}
		},
		{
			"scope": "namespace",
			"settings": {
				"foreground": "#FFF"
			}
		},
		{
			"scope": "number",
			"settings": {
				"foreground": "#FFF"
			}
		},
		{
			"scope": "operator",
			"settings": {
				"foreground": "#FFF"
			}
		},
		{
			"scope": "parameter",
			"settings": {
				"foreground": "#FFF"
			}
		},
		{
			"scope": "property",
			"settings": {
				"foreground": "#FFF"
			}
		},
		{
			"scope": "regexp",
			"settings": {
				"foreground": "#FFF"
			}
		},
		{
			"scope": "storage",
			"settings": {
				"foreground": "#d692bf"
			}
		},
		{
			"scope": "string",
			"settings": {
				"foreground": "#6fbe95"
			}
		},
		{
			"scope": "struct",
			"settings": {
				"foreground": "#FFF"
			}
		},
		{
			"scope": "support",
			"settings": {
				"foreground": "#f183a7"
			}
		},
		{
			"scope": "type",
			"settings": {
				"foreground": "#FFF"
			}
		},
		{
			"scope": "typeParameter",
			"settings": {
				"foreground": "#FFF"
			}
		},
		{
			"scope": "variable",
			"settings": {
				"foreground": "#c6a670"
			}
		}
	]
}