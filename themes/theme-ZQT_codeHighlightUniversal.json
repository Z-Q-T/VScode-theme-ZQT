{
	"$schema": "vscode://schemas/color-theme",
	"name": "Universal Code Highlight - ZQT Theme",
	"//description": "通用的代码高亮",
	"include": "./theme-ZQT_interface.json",
	"tokenColors": [
		{
			"scope": "class",
			"settings": {
				"foreground": "#FFF"
			}
		},
		{
			"scope": "comment",
			"settings": {
				"foreground": "#7f7f7f"
			}
		},
		{
			"scope": "invalid",
			"settings": {
				"foreground": "#ff0000"
			}
		},
		{
			"scope": "constant.numeric",
			"settings": {
				"foreground": "#14bbc2"
			}
		},
		{
			"scope": "constant.character",
			"settings": {
				"foreground": "#57bc8a"
			}
		},
		{
			"scope": "constant.language",
			"settings": {
				"foreground": "#ea7ca1"
			}
		},
		{
			"scope": "decorator",
			"settings": {
				"foreground": "#FFF"
			}
		},
		{
			"scope": "entity",
			"settings": {
				"foreground": "#e38d3d"
			}
		},
		{
			"scope": "entity.name.function",
			"settings": {
				"foreground": "#e38d3d"
			}
		},
		{
			"scope": "entity.name.type",
			"settings": {
				"foreground": "#e38d3d"
			}
		},
		{
			"scope": "entity.name.tag",
			"settings": {
				"foreground": "#e38d3d"
			}
		},
		{
			"scope": "entity.other.attribute-name",
			"settings": {
				"foreground": "#e38d3d"
			}
		},
		{
			"scope": "enum",
			"settings": {
				"foreground": "#FFF"
			}
		},
		{
			"scope": "enumMember",
			"settings": {
				"foreground": "#FFF"
			}
		},
		{
			"scope": "event",
			"settings": {
				"foreground": "#FFF"
			}
		},
		{
			"scope": "function",
			"settings": {
				"foreground": "#FFF"
			}
		},
		{
			"scope": "interface",
			"settings": {
				"foreground": "#FFF"
			}
		},
		{
			"scope": "keyword",
			"settings": {
				"foreground": "#ef7e80"
			}
		},
		{
			"scope": "keyword.control",
			"settings": {
				"foreground": "#ef7e80"
			}
		},
		{
			"scope": "keyword.operator",
			"settings": {
				"foreground": "#ed835e"
			}
		},
		{
			"scope": "label",
			"settings": {
				"foreground": "#FFF"
			}
		},
		{
			"scope": "macro",
			"settings": {
				"foreground": "#FFF"
			}
		},
		{
			"scope": "meta",
			"settings": {
				"foreground": "#3faff3"
			}
		},
		{
			"scope": "method",
			"settings": {
				"foreground": "#FFF"
			}
		},
		{
			"scope": "namespace",
			"settings": {
				"foreground": "#FFF"
			}
		},
		{
			"scope": "number",
			"settings": {
				"foreground": "#FFF"
			}
		},
		{
			"scope": "operator",
			"settings": {
				"foreground": "#FFF"
			}
		},
		{
			"scope": "parameter",
			"settings": {
				"foreground": "#FFF"
			}
		},
		{
			"scope": "property",
			"settings": {
				"foreground": "#FFF"
			}
		},
		{
			"scope": "storage",
			"settings": {
				"foreground": "#cf8cb8"
			}
		},
		{
			"scope": "string",
			"settings": {
				"foreground": "#77b493"
			}
		},
		{
			"scope": "string.regexp",
			"settings": {
				"foreground": "#FFF"
			}
		},
		{
			"scope": "string.interpolated",
			"settings": {
				"foreground": "#3fbf86"
			}
		},
		{
			"scope": "struct",
			"settings": {
				"foreground": "#FFF"
			}
		},
		{
			"scope": "support",
