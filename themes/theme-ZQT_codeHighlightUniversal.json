{
	"$schema": "vscode://schemas/color-theme",
	"name": "Universal Code Highlight - ZQT Theme",
	"//description": "通用的代码高亮",
	"include": "./theme-ZQT_interface.json",
	"tokenColors": [
		{
			"scope": "comment",
			"settings": {
				"foreground": "#636363"
			}
		},
		{
			"scope": "punctuation.definition.comment",
			"settings": {
				"foreground": "#636363"
			}
		},
		{
			"scope": "invalid",
			"settings": {
				"foreground": "#fc4734"
			}
		},
		{
			"scope": "entity.name.function",
			"settings": {
				"foreground": "#bf9f6a"
			}
		},
		{
			"scope": "support.function",
			"settings": {
				"foreground": "#d2a249"
			}
		},
		{
			"scope": "variable.language",
			"settings": {
				"foreground": "#6fbe95"
			}
		},
		{
			"scope": "variable.parameter",
			"settings": {
				"foreground": "#96b874"
			}
		},
		{
			"scope": "variable",
			"settings": {
				"foreground": "#98ad85"
			}
		},
		{
			"scope": "support.variable",
			"settings": {
				"foreground": "#92ba66"
			}
		},
		{
			"scope": "constant.language",
			"settings": {
				"foreground": "#59aaf8"
			}
		},
		{
			"scope": "constant.numeric",
			"settings": {
				"foreground": "#c690ec"
			}
		},
		{
			"scope": "string",
			"settings": {
				"foreground": "#db91b4"
			}
		},
		{
			"scope": "string.regexp",
			"settings": {
				"foreground": "#ec84b7"
			}
		},
		{
			"scope": [
				"string.interpolated",
				"string.template"
			],
			"settings": {
				"foreground": "#e57db1"
			}
		},
		{
			"scope": [
				"constant.character",
				"punctuation.definition.entity"
			],
			"settings": {
				"foreground": "#e38ab5"
			}
		},
		{
			"scope": "keyword.control",
			"settings": {
				"foreground": "#e68485"
			}
		},
		{
			"scope": "keyword.operator",
			"settings": {
				"foreground": "#d99c68"
			}
		},
		{
			"scope": "storage.type",
			"settings": {
				"foreground": "#a39adf"
			}
		},
		{
			"scope": "support.type",
			"settings": {
				"foreground": "#aa9afc"
			}
		},
		{
			"scope": "entity.name.type",
			"settings": {
				"foreground": "#86acea"
			}
		},
		{
			"scope": "punctuation.definition",
			"settings": {
				"foreground": "#75acd2"
			}
		},
		{
			"scope": "punctuation.separator",
			"settings": {
				"foreground": "#bebebe"
			}
		},
		{
			"scope": "keyword",
			"settings": {
				"foreground": "#f889ad"
			}
		},
		{
			"scope": "storage",
			"settings": {
				"foreground": "#f889ad"
			}
		},
		{
			"scope": "support",
			"settings": {
				"foreground": "#f889ad"
			}
		}
	]
}