{
	"$schema": "vscode://schemas/color-theme",
	"name": "Universal Code Highlight - ZQT Theme",
	"//description": "通用的代码高亮",
	"include": "./theme-ZQT_interface.json",
	"tokenColors": [
		{
			"scope": "comment",
			"settings": {
				"foreground": "#636363"
			}
		},
		{
			"scope": "punctuation.definition.comment",
			"settings": {
				"foreground": "#636363"
			}
		},
		{
			"scope": "invalid",
			"settings": {
				"foreground": "#fc4734"
			}
		},
		{
			"scope": "entity.name.function",
			"settings": {
				"foreground": "#bf9f6a"
			}
		},
		{
			"scope": "support.function",
			"settings": {
				"foreground": "#d9a02c"
			}
		},
		{
			"scope": "variable.language",
			"settings": {
				"foreground": "#6fbe95"
			}
		},
		{
			"scope": "variable.parameter",
			"settings": {
				"foreground": "#9ab580"
			}
		},
		{
			"scope": "variable",
			"settings": {
				"foreground": "#98ad85"
			}
		},
		{
			"scope": "support.variable",
			"settings": {
				"foreground": "#92ba66"
			}
		},
		{
			"scope": "constant.language",
			"settings": {
				"foreground": "#6fa4fc"
			}
		},
		{
			"scope": "constant.numeric",
			"settings": {
				"foreground": "#cc91da"
			}
		},
		{
			"scope": "string",
			"settings": {
				"foreground": "#bf96b0"
			}
		},
		{
			"scope": "string.regexp",
			"settings": {
				"foreground": "#d686bc"
			}
		},
		{
			"scope": [
				"string.interpolated",
				"string.template"
			],
			"settings": {
				"foreground": "#d686bc"
			}
		},
		{
			"scope": [
				"constant.character",
				"punctuation.definition.entity"
			],
			"settings": {
				"foreground": "#d686bc"
			}
		},
		{
			"scope": "keyword.control",
			"settings": {
				"foreground": "#e183a1"
			}
		},
		{
			"scope": "keyword.operator",
			"settings": {
				"foreground": "#d59c7d"
			}
		},
		{
			"scope": "storage.type",
			"settings": {
				"foreground": "#ac9ad0"
			}
		},
		{
			"scope": "support.type",
			"settings": {
				"foreground": "#b995f6"
			}
		},
		{
			"scope": "entity.name.type",
			"settings": {
				"foreground": "#86acea"
			}
		},
		{
			"scope": "punctuation.definition",
			"settings": {
				"foreground": "#7da4c0"
			}
		},
		{
			"scope": "punctuation.separator",
			"settings": {
				"foreground": "#bebebe"
			}
		}
	]
}